Proceso Ejercicio_015
	
	Definir distanciaKm, velocidadProm, tiempoHs Como Real;
	
	Escribir "Ingrese la distancia entre Ciudades: ";
	Leer distanciaKm;
	Escribir "Ingrese la velocidad Promedio: ";
	Leer velocidadProm;
	
	tiempoHs = distanciaKm / velocidadProm;
	
	Escribir "Su Recorrio : ", distanciaKm,  "km en ", tiempoHs, "hs";
	
FinProceso
