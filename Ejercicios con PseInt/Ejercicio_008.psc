Proceso Ejercicio_008
	Definir empleado, hsTrabajadas, totalPagar Como Entero;
	
	Escribir 'Ingrese la cantidad de hs Trabajadas: ';
	Leer hsTrabajadas;
	Escribir 'Ingrese cuanto cobra por hora: $';
	Leer empleado;
	
	totalPagar <- empleado * hsTrabajadas;
	
	Escribir 'El total a pagar es: $',totalPagar;
	
FinProceso
